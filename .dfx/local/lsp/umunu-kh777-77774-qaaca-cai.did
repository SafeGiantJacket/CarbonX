type Batch = 
 record {
   amount: nat;
   creator: principal;
   id: text;
   metadata: text;
   owner: principal;
 };
service : {
  getBatch: (id: text) -> (opt Batch) query;
  listBatches: () -> (vec Batch) query;
  registerBatch: (id: text, amount: nat, metadata: text) -> (bool);
  transferBatch: (id: text, newOwner: principal) -> (bool);
}
