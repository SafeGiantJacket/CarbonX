type Retirement = service {
  getAllRetirements : () -> (vec RetirementRecord) query;
  getUserRetirements : (principal) -> (vec RetirementRecord) query;
  retire : (nat, text) -> (bool);
};
type RetirementRecord = record {
  user : principal;
  timestamp : nat;
  amount : nat;
  reason : text;
};
service : () -> Retirement