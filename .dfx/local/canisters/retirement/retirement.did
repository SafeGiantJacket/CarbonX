type Retired = 
 record {
   amount: nat;
   id: text;
   metadata: text;
   owner: principal;
   timestamp: nat;
 };
service : {
  getRetired: (id: text) -> (opt Retired) query;
  listRetired: () -> (vec Retired) query;
  retireCredit: (id: text, amount: nat, metadata: text, timestamp: nat) ->
   (bool);
}
