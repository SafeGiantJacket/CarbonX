type Listing = 
 record {
   active: bool;
   creditId: text;
   id: text;
   price: nat;
   seller: principal;
 };
service : {
  createListing: (id: text, creditId: text, price: nat) -> (bool);
  deactivateListing: (id: text) -> (bool);
  getListing: (id: text) -> (opt Listing) query;
  listActive: () -> (vec Listing) query;
}
