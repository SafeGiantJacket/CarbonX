type Marketplace = 
 service {
   buy: (id: nat, quantity: nat) -> (bool);
   getListing: (id: nat) -> (opt Listing) query;
   getListings: () -> (vec Listing) query;
   list: (amount: nat, pricePerUnit: nat) -> (nat);
 };
type Listing = 
 record {
   amount: nat;
   id: nat;
   pricePerUnit: nat;
   seller: principal;
 };
service : () -> Marketplace
